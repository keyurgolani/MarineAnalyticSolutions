					}
				}
			},
			yAxis: {
				min: 0,
				title: {
					text: 'Total Delivery Area Wise'
				}
			},
			legend: {
				enabled: false
			},
			tooltip: {
				pointFormat: 'Average Revenue Per day: <b>{point.y:.1f} millions</b>'
			},
			series: [{
				name: 'Population',
				data: b,
				dataLabels: {
					enabled: true,
					rotation: -90,
					color: '#FFFFFF',
					align: 'right',
					format: '{point.y:.1f}', // one decimal
					y: 10, // 10 pixels down from the top
					style: {
						fontSize: '13px',
						fontFamily: 'Verdana, sans-serif'
					}
				}
			}]
		});

		function processData(data) {
			var newData = [];
			for(var i = 0 ; i < data.length ; i++) {
				newData.push([data[i].id,data[i].total]);
			}
			return newData;
		}
	}

</script>

